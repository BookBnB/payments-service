{
  "name": "template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "devStart": "nodemon --exec 'ts-node --compiler ttypescript' ./src/App.ts",
    "start": "dotenv -- typeorm migration:run && node ./build/App.js",
    "build": "npm run clean && ttsc",
    "clean": "rm -rf build",
    "test": "npm run test:unit && npm run test:aceptance",
    "test:unit": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.ts'",
    "test:aceptance": "node -r esm ./node_modules/.bin/cucumber-js --publish-quiet",
    "test:coverage": "nyc --nycrc-path config/nycrc-config.json npm run test",
    "typeorm": "dotenv -- ts-node ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:run": "npm run typeorm migration:run --",
    "migration:revert": "npm run typeorm migration:revert --"
  },
  "author": "seblaz mpicco FrancoMaz franfusaro",
  "license": "ISC",
  "dependencies": {
    "@truffle/hdwallet-provider": "^1.1.1",
    "@wessberg/di": "^2.0.3",
    "@wessberg/di-compiler": "^2.1.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "class-validator-jsonschema": "^2.0.3",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "dotenv-expand": "^5.1.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "log4js": "^6.3.0",
    "multer": "^1.4.2",
    "pg": "^8.4.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.6",
    "routing-controllers-openapi": "^2.1.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.28",
    "web3": "^1.3.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.0.0-rc.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.13",
    "@types/express": "^4.17.8",
    "@types/mocha": "^8.0.3",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.11.8",
    "@types/swagger-ui-express": "^4.1.2",
    "chai": "^4.2.0",
    "codecov": "^3.8.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "sinon": "^9.2.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.0.3"
  }
}
