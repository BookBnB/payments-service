{
  "name": "template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "devStart": "npm run migration:run && nodemon --watch src --exec 'node -r @wessberg/di-compiler/register' ./src/App.ts",
    "start": "dotenv -- typeorm migration:run && node ./build/App.js",
    "build": "npm run clean && ttsc",
    "build:watch": "npm run clean && ttsc --watch --inlineSourceMap",
    "clean": "rm -rf build",
    "test": "npm run test:unit && npm run test:aceptance",
    "test:unit": "mocha --config config/.mocharc.json",
    "test:unit:fast": "mocha --config config/.mocharc.fast.json",
    "test:aceptance": "cucumber-js --require-module @wessberg/di-compiler/register features/**/*.feature -r features/hooks.ts -r features/**/steps/*.ts --format progress-bar --exit --tags 'not @wip'",
    "test:aceptance:fast": "ROOT_DIR=./build/src cucumber-js features/**/*.feature -r build/features/hooks.js -r build/features/**/steps/*.js --format progress-bar --exit --tags 'not @wip'",
    "test:coverage": "rm -rf coverage && nyc --nycrc-path config/nycrc-config.json npm run test",
    "typeorm": "dotenv -- ts-node ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run --",
    "migration:revert": "npm run typeorm migration:revert --"
  },
  "author": "seblaz mpicco FrancoMaz franfusaro",
  "license": "ISC",
  "dependencies": {
    "@truffle/hdwallet-provider": "^1.1.1",
    "@wessberg/di": "^2.0.3",
    "@wessberg/di-compiler": "^2.1.1",
    "algoliasearch": "^4.6.0",
    "axios": "^0.21.0",
    "bignumber.js": "^9.0.1",
    "bip39": "^3.0.3",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "class-validator-jsonschema": "^2.0.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "dotenv-expand": "^5.1.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "geojson": "^0.5.0",
    "http-proxy-middleware": "^1.0.6",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "multer": "^1.4.2",
    "nanoid": "^3.1.16",
    "pg": "^8.4.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.6",
    "routing-controllers-openapi": "^2.1.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.28",
    "web3": "^1.3.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.13",
    "@types/chai-subset": "^1.3.3",
    "@types/cors": "^2.8.8",
    "@types/cucumber": "^6.0.1",
    "@types/express": "^4.17.8",
    "@types/geojson": "^7946.0.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.0.3",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.11.8",
